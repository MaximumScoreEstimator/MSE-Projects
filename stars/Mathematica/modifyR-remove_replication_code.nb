          RowBox[{"Table", "[", 
           RowBox[{"0", ",", 
            RowBox[{"{", 
             RowBox[{"noU", "[", 
              RowBox[{"[", "m", "]"}], "]"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"Length", "[", "d", "]"}], "}"}]}], "]"}]}], ";", 
         RowBox[{"Cmate", "[", "matchMatrix", "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"CmatchMatrix", "[", 
