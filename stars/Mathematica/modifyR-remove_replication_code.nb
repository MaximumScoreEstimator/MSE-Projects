  3.9019542399884377`*^9}, {3.9019542869865723`*^9, 3.90195428731686*^9}, {
  3.9038619422877283`*^9, 3.903861942927023*^9}, {3.904217049821595*^9, 
  3.904217051711524*^9}, {3.904217088422033*^9, 3.904217114470354*^9}, {
